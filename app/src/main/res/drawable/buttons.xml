<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <item android:state_pressed="true" tools:ignore="StateListReachable">
        <shape android:innerRadius="16dp">
            <corners android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp" android:topLeftRadius="8dp" android:topRightRadius="8dp" />
            <solid android:color="@color/button_dark" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:innerRadius="16dp">
            <corners android:bottomLeftRadius="8dp" android:bottomRightRadius="8dp" android:topLeftRadius="8dp" android:topRightRadius="8dp" />
            <solid android:color="@color/button" />
        </shape>
    </item>
</selector>